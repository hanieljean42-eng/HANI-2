{
  "expo": {
    "name": "HANI 2",
    "slug": "loveapp2026",
    "version": "5.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B9D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.loveapp.couple2026",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserNotificationsUsageDescription": "Nous utilisons les notifications pour vous alerter des messages de votre partenaire."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF6B9D"
      },
      "package": "com.loveapp.couple2026",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FF6B9D",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#FF6B9D",
      "androidMode": "default",
      "androidCollapsedTitle": "HANI 2 ðŸ’•"
    },
    "owner": "ddaq",
    "extra": {
      "eas": {
        "projectId": "87b635c7-a516-44d2-a9b4-8783d45c6cf4"
      }
    }
  }
}
